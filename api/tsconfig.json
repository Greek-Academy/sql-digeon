{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "es6"
    ],
    "module": "NodeNext",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
}
